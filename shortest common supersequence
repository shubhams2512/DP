#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int lcs_rec(char str1[],char str2[],int m,int n);
int max(int a ,int b);
int max(int a,int b)
{
    return (a>b)?a:b;
}

int main()
{
    int k,m,n;
    char str1[1000],str2[1000];
    scanf("%s",str1);
    scanf("%s",str2);
    m=strlen(str1);
    n=strlen(str2);
    printf("%d",lcs_rec(str1,str2,m,n));
}
int lcs_rec(char str1[],char str2[],int m,int n)
{
    int t[m+1][n+1],i,j;
    for(i=0;i<m+1;i++)
    {
        for(j=0;j<n+1;j++)
        {
            if(i==0||j==0)
            {
                t[i][j]=0;
            }
        }
    }
     for(i=1;i<m+1;i++)
    {
        for(j=1;j<n+1;j++)
        {
            if(str1[i-1]==str2[j-1])
            {
                t[i][j]=1+t[i-1][j-1];
            }
            else
            {
                t[i][j]=max(t[i-1][j],t[i][j-1]);
            }
            
        }
    }
    return ((m+n)-t[m][n]);
}
