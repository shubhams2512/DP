#include<stdio.h>
int sumwithdiff(int att[],int n,int sum);

int main()
{
    int k,arr[1000],n,diff,sum=0;
    scanf("%d",&n);
    scanf("%d",&diff);
    for(k=0;k<n;k++)
    {
        scanf("%d",&arr[k]);
    }
    for(k=0;k<n;k++)
    {
        sum+=arr[k];
    }
    sum=(sum+diff)/2;
    
    printf("%d",sumwithdiff(arr,n,sum));
}
int sumwithdiff(int arr[],int n,int sum)
{
    int i,j;
    
    int t[n+1][sum+1];
    for(i=0;i<n+1;i++)
    {
        for(j=0;j<sum+1;j++)
        {
            if(i==0)
            {
                t[i][j]=0;
            }
            if(j==0)
            {
                t[i][j]=1;
            }
        }
    }
    for(i=1;i<n+1;i++)
    {
        for(j=1;j<sum+1;j++)
        {
            if(arr[i-1]<=j)
            {
                t[i][j]=t[i-1][j]+t[i-1][j-arr[i-1]];
            }
            else
            {
                t[i][j]=t[i-1][j];
            }
        }
    }
    for(i=1;i<n+1;i++)
    {
        for(j=1;j<sum+1;j++)
        {printf("%d",t[i][j]);
        }printf("\n");
    }
    
    return t[n][sum];
}
    
    
    
    
